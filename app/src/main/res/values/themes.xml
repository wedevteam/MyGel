<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyGel" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#d7dfe4</item>
        <item name="colorPrimaryVariant">#5b6871</item>
        <item name="colorOnPrimary">@color/black</item>


        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/white</item>
        <item name="android:text">@color/black</item>
    </style>

    <style name="HintTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/button</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold|italic</item>
    </style>
</resources>